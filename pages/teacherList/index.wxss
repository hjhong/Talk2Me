/* pages/teacherList/index.wxss */
.teacher-list .search-bar{
  display: flex;
  flex-direction: row;
  background: #fff;
  border-bottom: 1rpx solid #eee;
}
.search-bar .b-item{
  flex-grow: 1;
  align-items: center;
  justify-content: center;
  display: flex;
}
.t-item{
  display: flex;
  flex-direction: row;
  background: #fff;
  padding: 32rpx;
  margin-top: 10rpx;
}
.list-wrap .avatar-img{
  width: 140rpx;
  height: 140rpx;
  border-radius: 4rpx;
}
.list-wrap .i-right{
   padding-left: 32rpx;
   font-size: 30rpx;
   color: #666;
}
.list-wrap .name{
  color: #333;
  font-size: 34rpx;
}
.list-wrap .star-info{
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
  font-size: 28rpx;
}
.list-wrap .star{
  width: 40rpx;
  height: 40rpx;
}
.course-name{
  color: #666;
  margin-right: 20rpx;
}
.go_jobadd {
  background: #4164e1;
  box-shadow: 0 8px 16px 0 rgba(65, 100, 225, 0.60);
  width: 112rpx;
  height: 112rpx;
  position: fixed;
  right: 4%;
  border-radius: 56rpx;
  bottom: 4%;
  font-size: 80rpx;
  line-height: 90rpx;
  text-align: center;
  color: #fff;
}

.go_jobadd image {
  width: 40rpx;
  height: 40rpx;
}